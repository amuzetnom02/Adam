{
  "buildCommand": "next build",
  "devCommand": "next dev",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "public": false,
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_THIRDWEB_CLIENT_ID": "${NEXT_PUBLIC_THIRDWEB_CLIENT_ID}",
    "THIRDWEB_SECRET_KEY": "${THIRDWEB_SECRET_KEY}"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
